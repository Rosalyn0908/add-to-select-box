<html>

<body>
<script src="http://ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>

<script>
$(document).ready(function(){
    $('button').click(function(){
        var addTo = parseInt($('textarea').val());    
        if (!isNaN(addTo)){
            $('select').append('<option>' + addTo + '</option>');
            }
    });
    
});
</script>

Type a number<textarea></textarea> <button>Submit</button>

<br>
<hr>
    <select multiple="5"></select>

</body>

</html>
